{
  "Certificate": {
    "Path": "#certificate_path#",
    "Password": "#certificate_password#"
  },
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=AspIdUsers.db;",
    "SQLConnection": "Server=#sql.server#;Initial Catalog=#sql.database#;User Id=#sql.user#;Password=#sql.password#;MultipleActiveResultSets=true;"
  },
  "Logs": {
    "MinLevel": "Info",
    "Path": "./Logs/log.ndjson"
  },
  "Google": {
    "ClientId": "#google.client.id#",
    "ClientSecret": "#google.client.secret#"
  },
  "IdentityServer": {
    "SpaClient": {
      "RedirectUris": "https://mvp-stack.com/callback.html, https://mvp-stack.com/renew-callback.html",
      "PostLogoutRedirectUris": "https://mvp-stack.com/signout-callback.html",
      "AllowedCorsOrigins": "https://mvp-stack.com"
    }
  }
}